{
    "name": "fadgram/laravel-bootstrap5-components",
    "description": "Reusable Bootstrap 5 components for Laravel projects",
    "homepage": "https://github.com/talalalmrka/laravel-bootstrap5-components",
    "version": "1.0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "Talal Almrka",
            "email": "talalminfo@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Fadgram\\LaravelBootstrap5Components\\": "src/"
        },
        "files": [
            "src/Helpers/helper.php",
            "src/Helpers/components.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fadgram\\LaravelBootstrap5Components\\Providers\\FadgramServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^9.1"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://repo.packagist.org"
        },
        "your-repo": {
            "type": "composer",
            "url": "https://api.github.com/repos/talalalmrka/fadgram-laravel-bootstrap5-components"
        }
    }
}
